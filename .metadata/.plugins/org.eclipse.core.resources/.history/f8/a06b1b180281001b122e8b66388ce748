package com.dipak.jan;

public class Array {

	private int[] arr;
	private int length;
	private int counter = 0;
	Array(int length) {
		this.length = length;
		arr = new int[this.length];
	}
	
	public void insert(int item) {
		this.arr[counter++] = item;
	}
	
	public String toString() {
		return java.util.Arrays.toString(arr);
		
	}
}
